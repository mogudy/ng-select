{
  "name": "@nbxx/nb-ng-select",
  "version": "2.0.0",
  "description": "Angular - nb - ng-select",
  "author": "@nbxx/nbxx",
  "repository": "git@github.com:mogudy/ng-select.git",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "license": "MIT",
  "keywords": [
    "angular",
    "angular6",
    "aot",
    "aot-compatible",
    "aot-compilation",
    "library",
    "ng",
    "typescript",
    "select"
  ],
  "scripts": {
    "build": "npm run build:esm && npm run build:umd",
    "build:esm": "gulp inline-templates && npm run ngcompile",
    "build:esm:watch": "gulp build:esm:watch",
    "build:umd": "webpack --config webpack-umd.config.ts",
    "build:umd:watch": "npm run build:umd -- --watch",
    "build:watch": "concurrently --raw \"npm run build:umd:watch\" \"npm run build:esm:watch\"",
    "ci": "npm run lint && npm run test && npm run build && npm run docs",
    "clean:all": "npm run clean:tmp && rimraf node_modules",
    "clean:tmp": "rimraf coverage dist tmp docs",
    "codecov": "cat coverage/lcov.info | codecov",
    "docs": "compodoc -p tsconfig.json -d docs --disableCoverage --disablePrivate --disableInternal --disableProtected --disableLifeCycleHooks",
    "explorer": "source-map-explorer ./dist/index.umd.js",
    "gh-pages": "rimraf docs && npm run docs && gh-pages -d docs",
    "lint": "npm run tslint 'src/**/*.ts'",
    "ngcompile": "node_modules/.bin/ngc -p tsconfig-aot.json",
    "postversion": "git push && git push --tags",
    "prebuild": "rimraf dist tmp",
    "prebuild:watch": "rimraf dist tmp",
    "prepublishOnly": "npm run ci",
    "preversion": "npm run ci",
    "pack": "npm run build && npm pack ./dist",
    "test": "karma start",
    "test:watch": "karma start --auto-watch --no-single-run",
    "tslint": "tslint"
  },
  "peerDependencies": {
    "@angular/common": "^6.1.0",
    "@angular/core": "^6.1.0"
  },
  "devDependencies": {
    "@angular/animations": "^6.1.0",
    "@angular/cli": "^6.1.1",
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/compiler-cli": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/forms": "^6.1.0",
    "@angular/http": "^6.1.0",
    "@angular/language-service": "^6.1.0",
    "@angular/platform-browser": "^6.1.0",
    "@angular/platform-browser-dynamic": "^6.1.0",
    "@compodoc/compodoc": "^1.0.4",
    "@types/jasmine": "^2.8.6",
    "@types/jasminewd2": "^2.0.3",
    "@types/karma": "^1.7.3",
    "@types/node": "^10.0.3",
    "codelyzer": "^4.1.0",
    "jasmine-core": "^2.9.1",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^3.0.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "1.4.1",
    "karma-jasmine": "1.1.1",
    "karma-jasmine-html-reporter": "0.2.2",
    "karma-firefox-launcher": "^1.1.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-remap-istanbul": "^0.6.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.3",
    "protractor": "^5.4.1",
    "ts-node": "^4.1.0",
    "tslint": "^5.9.1",
    "typescript": "2.9.2",
    "rxjs": "6.2.2",
    "@types/webpack": "^3.8.1",
    "@types/webpack-env": "^1.13.6",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "3.4.0",
    "classlist-polyfill": "1.0.3",
    "codecov": "^2.2.0",
    "concurrently": "^3.4.0",
    "css-loader": "^0.28.1",
    "gh-pages": "^1.0.0",
    "gulp": "^3.9.1",
    "gulp-inline-ng2-template": "^4.0.0",
    "istanbul-instrumenter-loader": "^3.0.0",
    "node-sass": "^4.7.2",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.1",
    "sass-loader": "6.0.5",
    "source-map-explorer": "^1.3.3",
    "to-string-loader": "^1.1.5",
    "ts-helpers": "^1.1.2",
    "tslint-loader": "^3.5.3",
    "typedoc": "^0.5.1",
    "url-loader": "^0.5.6",
    "webpack": "^3.8.1",
    "webpack-angular-externals": "^1.0.2",
    "webpack-bundle-analyzer": "^2.9.2",
    "webpack-dev-server": "^2.9.1",
    "webpack-rxjs-externals": "^1.0.0",
    "zone.js": "0.8.16"
  },
  "engines": {
    "node": ">= 8.9.4",
    "npm": ">= 5.6.0"
  }
}
